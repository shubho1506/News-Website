<div class="author">
    <h1 class="author_heading">
        Search Results
    </h1>

    <section class="articles">
        <ul class="articles_ul">
            <% data.forEach(post => { %>
            <li>
                <a href="/post/<%= post._id%>">
                    <span><%- post.title %></span>
                    <span class="article_list_date"><%= post.updatedAt.toDateString() %></span>
                </a>
            </li>
            <% }) %>
        </ul>
    </section>
</div>